@model StoneDesafio.Models.FaseCampeonato


@{
    ViewData["Title"] = "Criar uma nova fase";

    

}

<h1>Criar uma nova fase</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="criar">
            
            
            
            <div class="form-group">
                <label asp-for="FaseAtualCampeonato" class="control-label"></label>
                <input asp-for="FaseAtualCampeonato" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Jogos" class="control-label"></label>
                <input asp-for="Jogos" class="form-control" />
            </div>
            <br>
            <div class="form-group">
                <input type="submit" value="Criar" class="btn btn-primary"/>
            </div>
        </form>
    </div>
</div>
<hr>
<br>
<div>
    <a asp-action="Index">Voltar</a>
</div>
